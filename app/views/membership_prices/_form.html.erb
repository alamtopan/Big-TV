    <%= f.text_field :price, class: "form-control" %>
  </div>
</div>
<div class="form-group">
  <%= f.label :total_period, class: "col-sm-2 control-label" %>
  <div class="col-sm-9">
    <%= f.text_field :total_period, class: "form-control" %>
  </div>
</div>
