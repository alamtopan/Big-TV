    <%= f.select :membership_id, options_from_collection_for_select(@membership_id, :id, :name), {prompt: "--Please Select Membership--"}, class: "form-control" %>
  </div>
</div>
<div class="form-group">
  <%= f.label :price, class: "col-sm-2 control-label" %>
